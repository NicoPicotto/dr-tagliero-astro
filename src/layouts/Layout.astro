---
// style import
import Navbar from "@/components/commons/navbar.astro";
import Footer from "@/components/commons/footer.astro";
import "@/styles/starwind.css";
---

<!doctype html>
<html lang='es'>
   <head>
      <meta charset='UTF-8' />
      <meta name='viewport' content='width=device-width' />
      <link rel='preconnect' href='https://fonts.googleapis.com' />
      <link rel='preconnect' href='https://fonts.gstatic.com' crossorigin />
      <link
         rel='preload'
         as='style'
         href='https://fonts.googleapis.com/css2?family=Montserrat:wght@400;500;600;700&display=swap'
      />
      <link
         rel='stylesheet'
         href='https://fonts.googleapis.com/css2?family=Montserrat:wght@400;500;600;700&display=swap'
         media='print'
         onload="this.media='all'"
      />
      <noscript>
         <link
            rel='stylesheet'
            href='https://fonts.googleapis.com/css2?family=Montserrat:wght@400;500;600;700&display=swap'
         />
      </noscript>
      <meta
         name='description'
         content='Matías Tagliero – Abogado MP 12345. Especializado en derecho civil, penal, laboral y familiar. Ofrecemos asesoría legal personalizada, elaboración de contratos y representación judicial en Argentina.'
      />
      <meta
         name='keywords'
         content='abogado, Matías Tagliero, MP 12345, derecho civil, derecho penal, derecho laboral, derecho familiar, asesoría legal, representación judicial, consultoría jurídica, contratos, litigios, abogado en Argentina'
      />
      <!-- <script
         async
         src='https://cdn.tina.io/assets/iframe-resizer/tinacms-cloudframe.js'
      ></script> -->
      <link rel='icon' type='image/svg+xml' href='/favicon.svg' />
      <meta name='generator' content={Astro.generator} />
      <title>Abog. Matías Tagliero - MP 12345</title>
   </head>
   <body>
      <Navbar />
      <slot />
      <Footer />
   </body>
</html>

<style>
   html,
   body {
      margin: 0;
   }
</style>
